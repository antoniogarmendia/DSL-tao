<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="runtimeLayers" nsURI="http://mondo.org/runtimeLayers" nsPrefix="rtlay">
  <eAnnotations source="http://www.eclipse.org/OCL/Import">
    <details key="rtpat" value="http://mondo.org/runtimePatterns#/"/>
  </eAnnotations>
  <eClassifiers xsi:type="ecore:EClass" name="LayerSet">
    <eStructuralFeatures xsi:type="ecore:EReference" name="background" lowerBound="1"
        eType="#//BackgroundLayer" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="userLayers" upperBound="-1"
        eType="#//UserLayer" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BackgroundLayer" eSuperTypes="#//Layer"/>
  <eClassifiers xsi:type="ecore:EClass" name="UserLayer" eSuperTypes="#//Layer"/>
  <eClassifiers xsi:type="ecore:EClass" name="Layer">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="order" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="activated" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="visualised" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="content" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="patternRole" upperBound="-1"
        eType="#//PatternRoleDependece"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="use" upperBound="-1" eType="#//Layer"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="PatternRoleDependece">
    <eStructuralFeatures xsi:type="ecore:EReference" name="use" upperBound="-1" eType="#//Layer"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="pattern" lowerBound="1"
        eType="ecore:EClass runtimePatterns.ecore#//PatternInstance"/>
  </eClassifiers>
</ecore:EPackage>
